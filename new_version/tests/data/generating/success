# --- DIALECT_ID INPUT OUTPUT

# --- atoms
1 ('seq',('group',(('atom','a'),))) a
1 ('seq',('group',(('atom','a'),('atom','b'),('atom','c'),('atom','d'),('atom','e')))) abcde

# --- one-quantifiers and meta symbols
1 ('seq',('group',(('atom','a'),('repeat',('atom','b'),0,1),('atom','c'),('any',)))) ab{0,1}c.

# --- alternative groups
1 ('seq',('group',(('alt',(('atom','a'),('repeat',('atom','b'),0,1),('atom','c'),('any',)),(('atom','d'),)),))) (ab{0,1}c.|d)
1 ('seq',('group',(('atom','a'),('atom','b'),('atom','c'),('group',(('alt',(('atom','c'),),(('atom','a'),)),))))) abc(c|a)

# --- groups and ranges
1 ('seq',('group',(('alt',(('atom','a'),('repeat',('atom','b'),0,1),('atom','c'),('any',)),(('range',0,9),('escape',('atom','[')))),))) (ab{0,1}c.|[0-9]\[)
1 ('seq',('group',(('group',(('alt',(('atom','a'),),(('atom','b'),)),('atom','c'))),))) (a|b)c
