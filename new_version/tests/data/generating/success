# --- TEST SET INFO:
# --- #IN: DIALECT_ID INPUT_AST
# --- #DELIMITER: \n
# --- #OUT: OUTPUT_REGEX

# --- #TEST_GROUP: ATOMS
1 ('seq',('group',(('atom','a'),)))
a

1 ('seq',('group',(('atom','a'),('atom','b'),('atom','c'),('atom','d'),('atom','e'))))
abcde

# --- #TEST_GROUP: ONE-QUANTIFIERS AND META SYMBOLS
1 ('seq',('group',(('atom','a'),('repeat',('atom','b'),0,1),('atom','c'),('any',))))
ab{0,1}c.

# --- #TEST_GROUP: ALTERNATIVE GROUPS
1 ('seq',('group',(('alt',(('atom','a'),('repeat',('atom','b'),0,1),('atom','c'),('any',)),(('atom','d'),)),)))
(ab{0,1}c.|d)

1 ('seq',('group',(('atom','a'),('atom','b'),('atom','c'),('group',(('alt',(('atom','c'),),(('atom','a'),)),)))))
abc(c|a)

# --- #TEST_GROUP: GROUPS AND RANGES
1 ('seq',('group',(('alt',(('atom','a'),('repeat',('atom','b'),0,1),('atom','c'),('any',)),(('range',0,9),('escape',('atom','[')))),)))
(ab{0,1}c.|[0-9]\[)

1 ('seq',('group',(('group',(('alt',(('atom','a'),),(('atom','b'),)),('atom','c'))),)))
(a|b)c
