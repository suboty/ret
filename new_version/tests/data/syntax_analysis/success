# --- #TEST_GROUP: ATOMS
# --- reg: a
['1.atom,a']
('seq',('atom','a'))

# --- reg: abcde
['1.atom,a','1.atom,b','1.atom,c','1.atom,d','1.atom,e']
('seq',('atom','a'),('atom','b'),('atom','c'),('atom','d'),('atom','e'))

# --- #TEST_GROUP: ONE-QUANTIFIERS AND META SYMBOLS
# --- reg: ab?c.
['1.atom,a','1.atom,b','1.0_or_1','1.atom,c','1.any']
('seq',('atom','a'),('repeat_0_1',('atom','b')),('atom','c'),('any',))

# --- #TEST_GROUP: ALTERNATIVE GROUPS
# --- reg: ab?c.|d
['1.atom,a','1.atom,b','1.0_or_1','1.atom,c','1.any','1.alt','1.atom,d']
('seq',('alt',(('atom','a'),('repeat_0_1',('atom','b')),('atom','c'),('any',)),(('atom','d'),)))

# --- reg: abc(c|a)b
['1.atom,a','1.atom,b','1.atom,c','1.start_group','1.atom,c','1.alt','1.atom,a','1.end_group']
('seq',('atom','a'),('atom','b'),('atom','c'),('alt',(('atom','c'),),(('atom','a'),)),('atom','b'))

# --- #TEST_GROUP: GROUPS AND RANGES
# --- reg: ab?c.|[0-9]\[
["1.atom,a","1.atom,b","1.0_or_1","1.atom,c","1.any","1.alt","1.start_range","1.atom,0","1.atom,-","1.atom,9","1.end_range","1.escape","1.start_range"]
('seq',('group',(('alt',(('atom','a'),('repeat_0_1',('atom','b')),('atom','c'),('any',)),(('range_0_9',('escape',('atom','['))),)),)))

# --- reg: (a|b)c
["1.start_group","1.atom,a","1.alt","1.atom,b","1.end_group","1.atom,c"]
('seq',('group',(('alt',('atom',a),('atom',b)))),('atom',c))