# for demonstration
python3 main.py -r "a|b{1,2}"
python3 main.py -r "ab?c.|[0-9]\["
python3 main.py -r "abc(c|a)."
python3 main.py -r "a|b"
python3 main.py -r "c(d|a).\{"

# from unit tests
python3 main.py -r "a"
python3 main.py -r "abcde"
python3 main.py -r "ab?c."
python3 main.py -r "ab*c."
python3 main.py -r "ab+c."
python3 main.py -r "ab?c.|d"
python3 main.py -r "abc(c|a)b"
python3 main.py -r "ab?c.|[0-9]\["
python3 main.py -r "(a|b)c"
python3 main.py -r "(a|b{1,2})c"